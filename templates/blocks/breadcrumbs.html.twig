{% block breadcrumbs %}
        <section class="page-title-section">
            <div class="container">
                <div class="row">
                    <div class="col-xs-12">
                        <div class="page-header-wrap">
                            <div class="page-header">
                                <h1>{{ nameContent }}</h1>
                            </div>
                            <ol class="breadcrumb">
                                {% for item in breadcrumbs %}
                                    {% if item.url is defined and item.url is not empty  %}
                                        <li><a href={{ item.url }}>{{ item.name }}</a></li>
                                    {% else %}
                                        <li class="active"><span>{{ item.name }}</span></li>
                                    {% endif %}
                                {% endfor %}
                            </ol>
                        </div>
                    </div>
                </div>
            </div>
        </section>
{% endblock %}